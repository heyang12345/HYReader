# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'HYReader' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
   pod "SnapKit", "~> 0.0"
   pod "JKCategories"
   pod 'Alamofire', '~> 3.0'
   #分享
   pod 'OpenShare'
   # 极光推送
   pod 'JPush'
  
   #抽屉
   pod 'MMDrawerController', '~> 0.5.7'
   pod 'WPAttributedMarkup'
   pod "MBProgressHUD"
   pod 'FDFullscreenPopGesture'
   pod 'DGElasticPullToRefresh', '1.0.3'
   pod  "YYModel"
   # 二维码
   pod 'swiftScan'
   # ReactiveCocoa
   # 这个框架是 GitHub 官方团队开源的一个用于 iOS 上的函数响应式编程框架。
   # 如果想要在 iOS 上使用 MVVM 模式，配合这个框架最合适
   pod 'ReactiveCocoa', '~> 4.0'
   # Mob产品公共库
   pod 'MOBFoundation_IDFA'
   # 短信发送 SDK
   pod 'SMSSDK'
  # Pods for HYReader
  

end
